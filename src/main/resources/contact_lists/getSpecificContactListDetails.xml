<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://ws.apache.org/ns/synapse" name="getSpecificContactListDetails">
    <parameter name="userId"/>
	<parameter name="contactlistId"/>
    <sequence>
     <property name="uri.var.userId" expression="$func:userId" />
	 <property name="uri.var.contactlistId" expression="$func:contactlistId" />
        <call>
            <endpoint>
                <http method="GET" uri-template="{uri.var.eventbrite.apiUrl}/v3/users/{uri.var.userId}/contact_lists/{uri.var.contactlistId}/contacts/?token={uri.var.eventbrite.accessToken}" />
                <property name="Accept" value="*/*" scope="transport"/>
            </endpoint>
        </call>
    </sequence>
</template>
